extends layout

block scripts
  

block content
  h1= title
  p Welcome to #{title}
  ul
  each person in people
    li= person.name
    input(type='checkbox', checked=person.vacation)
  
  button(onclick=fs.serverGetPairs()) Server Generate pairs
  button(onclick=fs.clientGetPairs()) Client generate pairs
  
  form(action='/pairs',method='post')
    input(type='submit',value='Generate Pairs')
  
  button(onclick="send()") jquery post